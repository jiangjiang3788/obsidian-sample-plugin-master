{"name": "styles",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "types": [
      "vite/client",
      "preact"
    ]
  },
  "include": [
    "src"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build"
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "dependencies": {
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.38.1",
    "preact": "^10.26.9",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "vite": "^7.0.5"
  }
}
